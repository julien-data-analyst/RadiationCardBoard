	partition Datum = calculated
		mode: import
		source =
				
				VAR DebDate = MIN(RadiationMeasures[dateAndTimeOfCreation_DateOnly])
				VAR FinDate = MAX(RadiationMeasures[dateAndTimeOfCreation_DateOnly])
				RETURN
				ADDCOLUMNS(CALENDAR(DebDate, FinDate),
				        "Annee", FORMAT([Date], "YYYY"),
				        "Trimestre", "T"&FORMAT([Date], "Q"),
				        "Numero du mois", MONTH([Date]),
				        "Mois", FORMAT([Date], "MMMM"),
				        "AnneeMois", FORMAT([Date], "YYYYMM"),
				        "Nom du mois avec année", FORMAT([Date], "MMMM YY"),
				        "Nom court du mois", FORMAT([Date], "MMM YY"),
				        "Numero du jour de la semaine", WEEKDAY([Date]),
				        "Jour de la semaine", FORMAT([Date], "dddd")
				)

	annotation PBI_Id = 2fb99cc90d1b4ad68f18d1bed3d8add1

